{
  "name": "create-issue-branch",
  "version": "1.0.0",
  "description": "Creates a new branch after assigning an issue",
  "author": "Rob van der Leek <robvanderleek@gmail.com> (https://twitter.com/robvanderleek)",
  "license": "ISC",
  "repository": "https://github.com//create-issue-branch.git",
  "homepage": "https://github.com//create-issue-branch",
  "bugs": "https://github.com//create-issue-branch/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "NODE_OPTIONS='--max_old_space_size=460' probot run ./src/probot.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "dist": "ncc build src/action.js"
  },
  "dependencies": {
    "@sentry/node": "^5.20.1",
    "aws-sdk": "^2.734.0",
    "bunyan-sentry-stream": "^1.2.1",
    "probot": "^9.13.0",
    "probot-actions-adapter": "^1.0.4"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.3",
    "jest": "^26.2.2",
    "nock": "^13.0.3",
    "nodemon": "^2.0.4",
    "smee-client": "^1.0.2",
    "standard": "^14.3.4"
  },
  "engines": {
    "node": ">= 14.1.x"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "/dist/*.js"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
